<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
 <mapper namespace="com.biz.friend.persistence.UserDao">
 
 	<insert id="userInsert">
 		INSERT INTO `tbl_friend_user`
			(`user_number`,
			`user_id`,
			`user_nick`,
			`user_pw`)
		VALUES
			(#{user_number},
			#{user_id},
			#{user_nick},
			#{user_pw});
 	</insert>
 
 </mapper> 